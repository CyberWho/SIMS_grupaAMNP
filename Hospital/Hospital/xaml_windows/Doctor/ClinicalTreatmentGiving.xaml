<Window x:Class="Hospital.xaml_windows.Doctor.ClinicalTreatmentGiving"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hospital.xaml_windows.Doctor"
        mc:Ignorable="d"
        Title="ClinicalTreatmentGiving" Height="450" Width="800">
    <Grid>
        <DatePicker Name="dp_startTime"  HorizontalAlignment="Left" Margin="239,134,0,0" VerticalAlignment="Top"/>
        <DatePicker Name="dp_endTime" HorizontalAlignment="Left" Margin="239,202,0,0" VerticalAlignment="Top"/>
        <ListBox Name="lb_rooms" HorizontalAlignment="Left" Height="147" Margin="467,134,0,0" VerticalAlignment="Top" Width="170" SelectionChanged="lb_rooms_SelectionChanged"/>
        <Button Content="Trazi sobe" HorizontalAlignment="Left" Margin="266,298,0,0" VerticalAlignment="Top" Width="75" Click="FindRoomsWithBeds"/>
        <Button Content="Zakazi" HorizontalAlignment="Left" Margin="467,298,0,0" VerticalAlignment="Top" Width="75" Click="startClinicalTreatment"/>
        <Button Content="Odustani" HorizontalAlignment="Left" Margin="266,340,0,0" Click="ReturnOption" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
