<Window x:Class="Hospital.xaml_windows.Manager.ManagerRenovations"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hospital.xaml_windows.Manager"
        mc:Ignorable="d"
        Title="ManagerRenovations" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="activeRenovationsBtn" Content="Aktivne renovacije" HorizontalAlignment="Left" Margin="206,202,0,0" VerticalAlignment="Top" Width="114" Click="activeRenovationsBtn_Click"/>
        <Button x:Name="scheduleRenovationBtn" Content="Zakažite renovaciju" HorizontalAlignment="Left" Margin="362,202,0,0" VerticalAlignment="Top" Width="122" Click="scheduleRenovationBtn_Click"/>
        <Button x:Name="renovationHistoryBtn" Content="Istorija renovacija" HorizontalAlignment="Left" Margin="206,251,0,0" VerticalAlignment="Top" Width="114" Click="renovationHistoryBtn_Click"/>
        <Button x:Name="backBtn" Content="Nazad" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="backBtn_Click"/>

    </Grid>
</Window>
